package basicjava;
import java.util.Scanner;
public class biodata{
public static void main(String[] args){
Scanner input=new Scanner(System.in);
double Fahr, cel ;
System.out.println("Enter the value of Fahrenheit :");
Fahr =input.nextDouble();

cel =(0.56) * Fahr +32 ;
System.out.println("Enter the value of celsius:" +cel);
}
}
//(ans) Enter the value of Fahrenheit :46
//Enter the value of celsius :57.760000000000005
